<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swipes">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
      >
        <activity android:name=".MainActivity" android:theme="@style/Theme.AppCompat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" android:theme="@style/Theme.AppCompat"/>
        <activity android:name=".ProfileActivity" android:theme="@style/Theme.AppCompat"/>
        <activity android:name=".FireBaseUsers" android:theme="@style/Theme.AppCompat"/>
        <activity android:name=".FriendlyMessage" android:theme="@style/Theme.AppCompat"/>
    </application>

</manifest>